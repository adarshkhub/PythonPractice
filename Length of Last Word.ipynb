   "source": [
    "# Given a string s consisting of some words separated by some number of spaces, \n",
    "#return the length of the last word in the string.\n",
    "\n",
    "\n",
    "s = \"Hello World        \"\n",
    "i = len(s) - 1\n",
    "j = 0\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "while s[i] == \" \":\n",
    "    i-=1\n",
    "\n",
    "while i >= 0 and s[i] != \" \":\n",
    "    j+=1\n",
    "    \n",
    "print(j)"
   ]
